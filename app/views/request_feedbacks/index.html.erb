<p id="notice"><%= notice %></p>

<h1>Request Feedbacks</h1>

<table>
  <thead>
    <tr>
      <th>Rating</th>
      <th>Comment</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @request_feedbacks.each do |request_feedback| %>
      <tr>
        <td><%= request_feedback.rating %></td>
        <td><%= request_feedback.comment %></td>
        <td><%= link_to 'Show', request_feedback %></td>
        <td><%= link_to 'Edit', edit_request_feedback_path(request_feedback) %></td>
        <td><%= link_to 'Destroy', request_feedback, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Request Feedback', new_request_feedback_path %>
